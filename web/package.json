    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "framer-motion": "^12.23.13",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
